{
  "name": "dnbhub",
  "version": "1.0.0",
  "private": true,
  "description": "Dnbhub - multimedia concentrator",
  "homepage": "https://dnbhub-a5d9c.firebaseapp.com",
  "bugs": "https://github.com/rfprod/dnbhub/issues",
  "repository": "https://github.com/rfprod/dnbhub",
  "license": "MIT",
  "author": "rfprod <support@dnbhub.com> (https://github.com/rfprod)",
  "main": "server.js",
  "scripts": {
    "build": "bash shell/build.sh",
    "build:prod": "bash shell/build.sh prod",
    "e2e": "ng e2e",
    "postinstall": "npx sort-package-json && npm run ngcc",
    "install:global": "bash shell/install.sh global",
    "install:project": "bash shell/install.sh project",
    "lint": "ng run dnbhub:lint && ng run dnbhub:e2e-lint",
    "lint:fix": "ng run dnbhub:lint --fix && ng run dnbhub:e2e-lint --fix",
    "lint:html": "npx prettier -c src/**/*.html",
    "lint:html:fix": "npx prettier -c --write src/**/*.html",
    "lint:precommit": "yarn lint",
    "ng": "ng",
    "ngcc": "ngcc --tsconfig tsconfig.base.json",
    "ngcc:ci": "ngcc --tsconfig tsconfig.base.json --async false",
    "reset-client-app-env": "npx ts-node -O '{ \"module\": \"commonjs\", \"typeRoots\": [\"node_modules/@types\"] }' src/set-env.ts --reset=true && ng lint --fix",
    "set-client-app-env": "npx ts-node -O '{ \"module\": \"commonjs\", \"typeRoots\": [\"node_modules/@types\"] }' src/set-env.ts",
    "set-env": "bash shell/set-env.sh",
    "prestart": "npm run set-client-app-env",
    "start": "node server.js & ng serve",
    "start:express:prod": "node server.js prod",
    "test": "ng test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:precommit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@angular/animations": "11.0.0",
    "@angular/cdk": "11.0.0",
    "@angular/common": "11.0.0",
    "@angular/compiler": "11.0.0",
    "@angular/core": "11.0.0",
    "@angular/fire": "6.0.5",
    "@angular/flex-layout": "11.0.0-beta.33",
    "@angular/forms": "11.0.0",
    "@angular/material": "11.0.0",
    "@angular/material-moment-adapter": "11.0.0",
    "@angular/platform-browser": "11.0.0",
    "@angular/platform-browser-dynamic": "11.0.0",
    "@angular/router": "11.0.0",
    "@ngxs/form-plugin": "3.7.0",
    "@ngxs/router-plugin": "3.7.0",
    "@ngxs/store": "3.7.0",
    "core-js": "3.7.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "firebase": "8.0.2",
    "firebase-tools": "8.16.1",
    "material-design-icon-fonts": "3.0.1",
    "moment": "2.29.1",
    "roboto-fontface": "0.10.0",
    "rxjs": "6.6.3",
    "web-animations-js": "2.3.2",
    "ws": "7.4.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "10.0.1",
    "@angular-devkit/build-angular": "0.1100.1",
    "@angular-eslint/builder": "0.6.0-beta.0",
    "@angular-eslint/eslint-plugin": "0.6.0-beta.0",
    "@angular-eslint/eslint-plugin-template": "0.6.0-beta.0",
    "@angular-eslint/template-parser": "0.6.0-beta.0",
    "@angular/cli": "11.0.1",
    "@angular/compiler-cli": "11.0.0",
    "@angular/language-service": "11.0.0",
    "@briebug/cypress-schematic": "4.3.0",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@cypress/webpack-preprocessor": "5.4.11",
    "@ngneat/until-destroy": "8.0.3",
    "@ngxs/logger-plugin": "3.7.0",
    "@types/core-js": "2.5.4",
    "@types/hammerjs": "2.0.36",
    "@types/jest": "26.0.15",
    "@types/jsdom": "16.2.5",
    "@types/node": "14.14.7",
    "@typescript-eslint/eslint-plugin": "4.7.0",
    "@typescript-eslint/parser": "4.7.0",
    "codelyzer": "6.0.1",
    "cypress": "5.6.0",
    "eslint": "7.13.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-compat": "3.8.0",
    "eslint-plugin-deprecation": "1.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-rxjs": "2.1.3",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "eslint-watch": "7.0.0",
    "jest": "26.6.3",
    "jest-preset-angular": "8.3.2",
    "prettier": "2.1.2",
    "prettier-stylelint": "0.4.2",
    "sort-json": "2.0.0",
    "sort-package-json": "1.46.1",
    "stylelint": "13.7.2",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-prettier": "1.1.2",
    "ts-jest": "26.4.4",
    "ts-loader": "8.0.11",
    "ts-node": "9.0.0",
    "typescript": "4.0.5",
    "webpack-bundle-analyzer": "4.1.0"
  },
  "engines": {
    "node": ">=14.8.0",
    "npm": ">=6.14.8"
  }
}
