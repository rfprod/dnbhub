{
  "name": "dnbhub",
  "version": "1.0.0",
  "private": true,
  "description": "Dnbhub - multimedia concentrator",
  "homepage": "https://dnbhub-a5d9c.firebaseapp.com",
  "bugs": "https://github.com/rfprod/dnbhub/issues",
  "repository": "https://github.com/rfprod/dnbhub",
  "license": "MIT",
  "author": "rfprod <connect@rfprod.tk> (https://github.com/rfprod)",
  "main": "server.js",
  "scripts": {
    "analyze:app": "ng build --stats-json --configuration analyze-bundle; npx webpack-bundle-analyzer ./dist/stats.json",
    "build": "bash shell/build.sh",
    "build:prod": "bash shell/build.sh prod",
    "e2e": "ng e2e",
    "install:global": "bash shell/install.sh global",
    "install:project": "bash shell/install.sh project",
    "install:safe": "yarn install --ignore-scripts",
    "postinstall:safe": "husky install && npx sort-package-json && yarn ngcc",
    "lint": "ng run dnbhub:lint && ng run dnbhub:e2e-lint",
    "lint:fix": "ng run dnbhub:lint --fix && ng run dnbhub:e2e-lint --fix",
    "lint:html": "npx prettier -c src/**/*.html",
    "lint:html:fix": "npx prettier -c --write src/**/*.html",
    "lint:precommit": "yarn lint",
    "ng": "ng",
    "ngcc": "ngcc --tsconfig tsconfig.json",
    "ngcc:ci": "ngcc --tsconfig tsconfig.json --async false",
    "postinstall": "husky install && npx sort-package-json && npm run ngcc",
    "prestart": "npm run set-client-app-env",
    "start": "node server.js & ng serve",
    "reset-client-app-env": "npx ts-node -O '{ \"module\": \"commonjs\", \"typeRoots\": [\"node_modules/@types\"] }' src/set-env.ts --reset=true && ng lint --fix",
    "set-client-app-env": "npx ts-node -O '{ \"module\": \"commonjs\", \"typeRoots\": [\"node_modules/@types\"] }' src/set-env.ts",
    "set-env": "bash shell/set-env.sh",
    "start:express:prod": "node server.js prod",
    "test": "ng test"
  },
  "dependencies": {
    "@angular/animations": "12.0.2",
    "@angular/cdk": "12.0.2",
    "@angular/common": "12.0.2",
    "@angular/compiler": "12.0.2",
    "@angular/core": "12.0.2",
    "@angular/fire": "6.1.5",
    "@angular/flex-layout": "12.0.0-beta.34",
    "@angular/forms": "12.0.2",
    "@angular/material": "12.0.2",
    "@angular/material-moment-adapter": "12.0.2",
    "@angular/platform-browser": "12.0.2",
    "@angular/platform-browser-dynamic": "12.0.2",
    "@angular/router": "12.0.2",
    "@ngxs/form-plugin": "3.7.2",
    "@ngxs/router-plugin": "3.7.2",
    "@ngxs/store": "3.7.2",
    "core-js": "3.13.1",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "express-session": "1.17.2",
    "firebase": "8.6.3",
    "firebase-tools": "9.12.0",
    "hammerjs": "2.0.8",
    "material-design-icon-fonts": "3.0.1",
    "moment": "2.29.1",
    "roboto-fontface": "0.10.0",
    "rxjs": "7.1.0",
    "web-animations-js": "2.3.2",
    "ws": "7.4.6",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "12.0.0",
    "@angular-devkit/build-angular": "12.0.2",
    "@angular-eslint/builder": "12.0.0",
    "@angular-eslint/eslint-plugin": "12.0.0",
    "@angular-eslint/eslint-plugin-template": "12.0.0",
    "@angular-eslint/template-parser": "12.0.0",
    "@angular/cli": "12.0.2",
    "@angular/compiler-cli": "12.0.2",
    "@angular/language-service": "12.0.2",
    "@briebug/cypress-schematic": "5.0.0",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@cypress/webpack-preprocessor": "5.9.0",
    "@ngneat/until-destroy": "8.0.4",
    "@ngxs/logger-plugin": "3.7.2",
    "@types/core-js": "2.5.4",
    "@types/hammerjs": "2.0.39",
    "@types/jest": "26.0.23",
    "@types/jsdom": "16.2.10",
    "@types/node": "15.6.1",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "codelyzer": "6.0.2",
    "cypress": "7.4.0",
    "eslint": "7.27.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-compat": "3.9.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-rxjs": "3.3.3",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-watch": "7.0.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "jest-preset-angular": "8.4.0",
    "prettier": "2.3.0",
    "prettier-stylelint": "0.4.2",
    "sort-json": "2.0.0",
    "sort-package-json": "1.50.0",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-prettier": "1.2.0",
    "ts-jest": "26.5.5",
    "ts-loader": "9.2.2",
    "ts-node": "10.0.0",
    "typescript": "4.2.4",
    "webpack-bundle-analyzer": "4.4.2"
  },
  "engines": {
    "node": ">=14.8.0",
    "npm": ">=6.14.8"
  }
}
