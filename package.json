  "scripts": {
    "build": "bash shell/build.sh",
    "build-prod": "bash shell/build.sh prod",
    "e2e": "ng e2e",
    "express-prod": "node server.js prod",
    "postinstall": "npx sort-package-json",
    "install-firebase": "bash shell/install.sh firebase",
    "install-global": "bash shell/install.sh global",
    "install-local": "bash shell/install.sh",
    "install-project": "bash shell/install.sh project",
    "lint": "ng lint",
    "ng": "ng",
    "ng-build": "ng build",
    "ng-build-prod": "ng build --prod",
    "ng-build-prod-firebase": "ng build --prod --baseHref /dnbhub/",
    "pre-e2e": "npm run update-webdriver",
    "reset-client-app-env": "npx ts-node -O '{ \"module\": \"commonjs\", \"typeRoots\": [\"node_modules/@types\"] }' src/set-env.ts --reset=true",
    "set-client-app-env": "npx ts-node -O '{ \"module\": \"commonjs\", \"typeRoots\": [\"node_modules/@types\"] }' src/set-env.ts",
    "set-env": "bash shell/set-env.sh",
    "prestart": "npm run set-client-app-env",
    "start": "node server.js & ng serve",
    "test": "ng test"
  },
